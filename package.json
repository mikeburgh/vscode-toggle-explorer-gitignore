{
  "name": "toggle-explorer-gitignore",
  "description": "",
  "version": "1.0.0",
  "activationEvents": [
    "*"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "toggle-explorer-gitignore.toggle",
        "title": "Toggle showing ignored files"
      },
      {
        "command": "toggle-explorer-gitignore.hide",
        "title": "Hide Git-ignored files"
      },
      {
        "command": "toggle-explorer-gitignore.show",
        "title": "Show Git-ignored files"
      }
    ]
  },
  "devDependencies": {
    "prettier": "^2.8.8"
  },
  "engines": {
    "vscode": "^1.73.0"
  }
}
